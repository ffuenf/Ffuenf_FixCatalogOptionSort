{
  "name": "Ffuenf/Ffuenf_FixCatalogOptionSort",
  "license": "MIT",
  "description": "Magento 1.9.* has a bug in that the configurable options are sorted by ID rather than position for the Configurable Product's front end view script.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    }
  ],
  "type": "magento-module",
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "map": [
      [
        "app/code/local/Ffuenf/FixCatalogOptionSort",
        "app/code/local/Ffuenf/FixCatalogOptionSort"
      ],
      [
        "app/etc/modules/Ffuenf_FixCatalogOptionSort.xml",
        "app/etc/modules/Ffuenf_FixCatalogOptionSort.xml"
      ]
    ]
  }
}